{
    // Editor 
    "breadcrumbs.enabled": false,
    "editor.smoothScrolling": true,
    "editor.formatOnSave": false,
    "editor.rulers": [
        80
    ],
    "editor.autoClosingBrackets": "never",
    "editor.autoClosingQuotes": "never",
    "editor.minimap.enabled": false,
    "editor.mouseWheelZoom": true,
    "editor.codeLens": false,
    "editor.fontFamily": "JetBrains Mono NL",
    "editor.fontLigatures": true,
    "editor.fontSize": 13,
    "editor.lineNumbers": "relative",
    "editor.bracketPairColorization.enabled": true,
    "editor.guides.bracketPairs": true,
    "editor.renderLineHighlight": "line",
    "editor.wordWrap": "off",
    "editor.tabCompletion": "on",
    "editor.tabSize": 4,
    "editor.detectIndentation": false,
    "editor.cursorBlinking": "solid",
    "editor.cursorSurroundingLines": 10,
    "editor.wordBasedSuggestions": "off",
    "editor.accessibilitySupport": "off",
    "editor.inlineSuggest.enabled": true,
    "editor.suggest.showKeywords": true,
    "editor.snippetSuggestions": "inline",
    "editor.quickSuggestions": {
        "comments": false,
        "strings": true
    },
    // Terminal 
    "terminal.integrated.enableMultiLinePasteWarning": "never",
    "terminal.integrated.gpuAcceleration": "on",
    "terminal.integrated.env.windows": {},
    "terminal.integrated.env.linux": {},
    // Emmet
    "emmet.showSuggestionsAsSnippets": true,
    "emmet.includeLanguages": {
        "javascript": "javascriptreact",
        "typescript": "javascriptreact",
    },
    "emmet.excludeLanguages": [
        "markdown",
        "typescript",
    ],
    "emmet.showAbbreviationSuggestions": false,
    "settingsSync.keybindingsPerPlatform": false,
    // File types
    "javascript.suggest.autoImports": true,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "javascript.suggest.paths": false,
    "[javascript]": {
        "editor.tabSize": 4,
        "editor.defaultFormatter": "esbenp.prettier-vscode",
    },
    "[javascriptreact]": {
        "editor.tabSize": 2
    },
    "typescript.suggest.autoImports": true,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "typescript.suggest.paths": false,
    "[typescript]": {
        "editor.tabSize": 4,
        "editor.defaultFormatter": "esbenp.prettier-vscode",
    },
    "[typescriptreact]": {
        "editor.tabSize": 2
    },
    "[html]": {
        "editor.tabSize": 2,
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "[css]": {
        "editor.tabSize": 4
    },
    "[php]": {
        "editor.tabSize": 4,
        "editor.defaultFormatter": "DEVSENSE.phptools-vscode"
    },
    "python.analysis.autoImportCompletions": true,
    "[python]": {
        "editor.tabSize": 4,
        "editor.defaultFormatter": "ms-python.vscode-pylance"
    },
    "css.enabledLanguages": [
        "html",
        "php"
    ],
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    /* Vim settings */
    "vim.ignorecase": false,
    "vim.highlightedyank.enable": true,
    "vim.highlightedyank.duration": 80,
    "vim.scroll": 10,
    "vim.leader": "<Space>",
    "vim.useSystemClipboard": true,
    "vim.timeout": 300,
    "vim.cursorStylePerMode.insert": "block-outline",
    "vim.cursorStylePerMode.normal": "block",
    "vim.cursorStylePerMode.visual": "block",
    "vim.cursorStylePerMode.visualblock": "block",
    "vim.cursorStylePerMode.visualline": "block",
    "vim.showcmd": true,
    "vim.hlsearch": true,
    "vim.incsearch": true,
    "vim.surround": true,
    "vim.sneak": true,
    "vim.easymotion": true,
    /* Vim keybinds */
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "g",
                "r"
            ],
            "commands": [
                "editor.action.goToReferences"
            ]
        },
        {
            "before": [
                "<leader>",
                "w"
            ],
            "commands": [
                ":w"
            ]
        },
        {
            "before": [
                "<leader>",
                "w",
                "r"
            ],
            "commands": [
                "save-as-root.saveFile"
            ]
        },
        {
            "before": [
                "<leader>",
                "f",
                "f"
            ],
            "commands": [
                "editor.action.formatDocument"
            ],
        },
        {
            "before": [
                "<leader>",
                "f",
                "p"
            ],
            "commands": [
                "prettier.forceFormatDocument"
            ]
        },
        {
            "before": [
                "Y"
            ],
            "after": [
                "y",
                "$"
            ]
        },
        {
            "before": [
                "z",
                "j"
            ],
            "after": [
                "o",
                "<ESC>",
                "k"
            ]
        },
        {
            "before": [
                "z",
                "k"
            ],
            "after": [
                "O",
                "<ESC>",
                "j"
            ]
        },
        {
            "before": [
                "<leader>",
                "h"
            ],
            "commands": [
                ":nohl",
                "notifications.clearAll"
            ]
        },
        {
            "before": [
                "<leader>",
                "y"
            ],
            "after": [
                "\"",
                "a",
                "y"
            ]
        },
        {
            "before": [
                "<leader>",
                "Y"
            ],
            "after": [
                "\"",
                "a",
                "y",
                "$"
            ]
        },
        {
            "before": [
                "<leader>",
                "d"
            ],
            "after": [
                "\"",
                "a",
                "d"
            ]
        },
        {
            "before": [
                "<leader>",
                "D"
            ],
            "after": [
                "\"",
                "a",
                "D"
            ]
        },
        {
            "before": [
                "<leader>",
                "c"
            ],
            "after": [
                "\"",
                "a",
                "c"
            ]
        },
        {
            "before": [
                "<leader>",
                "C"
            ],
            "after": [
                "\"",
                "a",
                "C"
            ]
        },
        {
            "before": [
                "<leader>",
                "p"
            ],
            "after": [
                "\"",
                "a",
                "p"
            ]
        },
        {
            "before": [
                "<leader>",
                "P"
            ],
            "after": [
                "\"",
                "a",
                "P"
            ]
        },
        {
            "before": [
                "<C-d>"
            ],
            "after": [
                "<C-d>",
                "z",
                "z"
            ]
        },
        {
            "before": [
                "<C-u>"
            ],
            "after": [
                "<C-u>",
                "z",
                "z"
            ]
        },
        {
            "before": [
                "<leader>",
                "e",
                "w"
            ],
            "commands": [
                "editor.emmet.action.wrapWithAbbreviation"
            ]
        },
        {
            "before": [
                "<leader>",
                "e",
                "r"
            ],
            "commands": [
                "editor.emmet.action.removeTag"
            ]
        },
        {
            "before": [
                "<leader>",
                "e",
                "m"
            ],
            "commands": [
                "editor.emmet.action.matchTag"
            ]
        },
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [
                ">"
            ],
            "after": [
                ">",
                "g",
                "v"
            ]
        },
        {
            "before": [
                "<"
            ],
            "after": [
                "<",
                "g",
                "v"
            ]
        },
        {
            "before": [
                "<leader>",
                "y"
            ],
            "after": [
                "\"",
                "a",
                "y"
            ]
        },
        {
            "before": [
                "<leader>",
                "d"
            ],
            "after": [
                "\"",
                "a",
                "d"
            ]
        },
        {
            "before": [
                "<leader>",
                "p"
            ],
            "after": [
                "\"",
                "a",
                "p"
            ]
        },
        {
            "before": [
                "<leader>",
                "e",
                "w"
            ],
            "commands": [
                "editor.emmet.action.wrapWithAbbreviation"
            ]
        },
        {
            "before": [
                "<leader>",
                "e",
                "r"
            ],
            "commands": [
                "editor.emmet.action.removeTag"
            ]
        },
        {
            "before": [
                "<leader>",
                "e",
                "m"
            ],
            "commands": [
                "editor.emmet.action.matchTag"
            ]
        },
    ],
    // Files
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        // "**/node_modules": true,
        "**/package-lock.json": true
    },
    "files.associations": {
        "*.css": "tailwindcss"
    },
    "files.simpleDialog.enable": true,
    "explorer.confirmDragAndDrop": true,
    "explorer.autoReveal": false,
    "explorer.compactFolders": false,
    "errorLens.removeLinebreaks": false,
    "errorLens.onSave": true,
    "git.openRepositoryInParentFolders": "never",
    "code-runner.runInTerminal": true,
    "search.quickOpen.includeHistory": false,
    "search.exclude": {
        "**/node_modules": true,
        "**/vendor": true,
    },
    "extensions.experimental.affinity": {
        "vscodevim.vim": 1
    },
    "extensions.ignoreRecommendations": true,
    "security.workspace.trust.untrustedFiles": "open",
    "security.workspace.trust.enabled": false,
    "telemetry.telemetryLevel": "off",
    // Window 
    "window.menuBarVisibility": "compact",
    "window.commandCenter": false,
    "window.restoreFullscreen": true,
    "window.newWindowDimensions": "maximized",
    "window.titleBarStyle": "custom",
    "window.customMenuBarAltFocus": false,
    "window.zoomLevel": 0,
    // Workbench 
    "workbench.activityBar.location": "top",
    "workbench.startupEditor": "none",
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.tree.indent": 16,
    "workbench.colorCustomizations": {
        // "editorCursor.foreground": "#fff",
        "editorCursor.foreground": "#ffde21",
    },
    "editor.tokenColorCustomizations": {
        "comments": "#6a9955",
    },
    "workbench.editor.empty.hint": "hidden",
    "workbench.iconTheme": "material-icon-theme",
    "projectManager.git.baseFolders": [
        "C:\\Users\\baki\\workspace"
    ],
    "projectManager.sortList": "Recent",
    "explorer.confirmPasteNative": false,
    "workbench.colorTheme": "Remedy - Dark (Straight)",
}